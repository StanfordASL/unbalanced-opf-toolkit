//*********************************************
// Transformer config Objects
//*********************************************

object transformer_configuration {
	name Xfrm_config_PV_1700011453;
	connect_type SINGLE_PHASE_CENTER_TAPPED;
	install_type POLETOP;
	power_rating 500000;
	primary_voltage 7274.613;
	secondary_voltage 120;
	resistance 0.09;
	reactance 1.81;
}


//*********************************************
// Transformer Objects
//*********************************************

object transformer {
	name Xfrm_1700011453_A;
	phases AS;
	from N_1700011453;
	to N_1700011453_sec_A;
	configuration Xfrm_config_PV_1700011453;
}

object transformer {
	name Xfrm_1700011453_B;
	phases BS;
	from N_1700011453;
	to N_1700011453_sec_B;
	configuration Xfrm_config_PV_1700011453;
}

object transformer {
	name Xfrm_1700011453_C;
	phases CS;
	from N_1700011453;
	to N_1700011453_sec_C;
	configuration Xfrm_config_PV_1700011453;
}


//*********************************************
// Triplex meter Objects
//*********************************************

object triplex_meter {
	name N_1700011453_sec_A;
	phases AS;
	nominal_voltage 120;
}

object triplex_meter {
	name N_1700011453_sec_B;
	phases BS;
	nominal_voltage 120;
}

object triplex_meter {
	name N_1700011453_sec_C;
	phases CS;
	nominal_voltage 120;
}

//*********************************************
// Inverter Objects
//*********************************************

object inverter {
	name inverter_N_1700011453_A;
